#include<iostream>
#include<iomanip>
using namespace std;
template <typename T>
void pM(T m,int a,int b=0,int w=0){
	cout<<setw(w)<<'#';
	for(int i=0;i<b;i++){
		cout<<setw(w)<<i;
	}
	cout<<endl;
	for(int i=0;i<a;i++){
		cout<<setw(w)<<i;
		for(int j=0;j<b;j++){
			cout<<setw(w)<<m[i][j];
		}
		cout<<endl;
	}
}
